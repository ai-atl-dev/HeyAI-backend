# GCP Configuration
GCP_PROJECT_ID=your-project-id
GCP_REGION=us-central1
GCP_PROJECT_NUMBER=123456789012

# Server Configuration
PORT=8080
ENVIRONMENT=development

# Twilio Configuration
TWILIO_ACCOUNT_SID=ACxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# BigQuery Configuration
BIGQUERY_DATASET=agent_data
BIGQUERY_CALLS_TABLE=calls
BIGQUERY_USAGE_TABLE=usage_history
BIGQUERY_PAYMENTS_TABLE=payments

# Firestore Configuration
FIRESTORE_COLLECTION=agents
FIRESTORE_SESSIONS_COLLECTION=sessions

# Vertex AI Configuration
VERTEX_AI_LOCATION=us-central1
VERTEX_AI_MODEL=gemini-2.5-flash
VERTEX_AI_ENDPOINT=

# OAuth Configuration
OAUTH_CLIENT_ID=your-oauth-client-id.apps.googleusercontent.com
OAUTH_CLIENT_SECRET=your-oauth-client-secret
OAUTH_REDIRECT_URL=http://localhost:8080/auth/callback
OAUTH_STATE_SECRET=random-secret-string-for-state-validation

# JWT Configuration
JWT_SECRET=your-jwt-secret-key-change-this-in-production
JWT_EXPIRATION=24h

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173

# Logging
LOG_LEVEL=info
LOG_FORMAT=json

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=1m

# External Agent API (Repository 1)
EXTERNAL_AGENT_API_URL=https://your-external-agent-api.com
EXTERNAL_AGENT_API_KEY=your-external-agent-api-key

# Sesame AI Configuration
SESAME_AI_API_KEY=your-sesame-ai-api-key
SESAME_AI_BASE_URL=https://api.sesame.ai/v1

# ElevenLabs Configuration
ELEVENLABS_API_KEY=your-elevenlabs-api-key
